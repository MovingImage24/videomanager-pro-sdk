{
  "name": "mi/videomanager-pro-sdk",
  "description": "SDK for the VideoManager Pro REST API",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "sdk",
    "videomanager",
    "videomanagerPro"
  ],
  "authors": [
    {
      "name": "Steve Reichenbach",
      "email": "steve.reichenbach@movingimage.com"
    }
  ],
  "minimum-stability": "beta",
  "extra": {
